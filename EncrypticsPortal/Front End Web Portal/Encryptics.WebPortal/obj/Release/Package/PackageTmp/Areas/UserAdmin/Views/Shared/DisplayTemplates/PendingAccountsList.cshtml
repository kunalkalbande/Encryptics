@model IEnumerable<Encryptics.WebPortal.Areas.UserAdmin.Models.PendingAccountsListItemModel>
<tr>
    <th class="selectColumn" style="text-align: center;"></th>
    <th class="userNameColumn"><a href="#" class="columnSort" data-column-name="Email">@Html.DisplayNameFor(m => m.UserName)</a></th>
    <th style="width: 20%" class="firstNameColumn"><a href="#" class="columnSort" data-column-name="First_Name">@Html.DisplayNameFor(m => m.FirstName)</a></th>
    <th style="width: 20%" class="lastNameColumn"><a href="#" class="columnSort" data-column-name="Last_Name">@Html.DisplayNameFor(m => m.LastName)</a></th>
    <th class="accountTypeColumn">Account Type</th>
</tr>
@foreach (var domain in Model)
{
    @Html.DisplayFor(m => domain)
}